{
    "name": "neuralpin/phpinventory",
    "description": "PHP Inventory Project",
    "type": "project",
    "license": "private",
    "authors": [
        {
            "name": "Ulises Rendon",
            "email": "fdulises@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": {
        "neuralpin/httprouter": {
            "type": "vcs",
            "url": "https://github.com/neuralpin/httprouter"
        },
        "neuralpin/passencryption": {
            "type": "vcs",
            "url": "https://github.com/neuralpin/neuralpin_passencryption"
        }
    },
    "require": {
        "php": "^8.3",
        "vlucas/phpdotenv": "5.6.x-dev",
        "neuralpin/httprouter": "v3.*",
        "neuralpin/passencryption": "v1.*"
    },
    "autoload": {
        "psr-4": {
            "Stradow\\": "src/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "7.2.x-dev",
        "laravel/pint": "dev-main",
        "phpunit/phpunit": "^12.0@dev",
        "phpstan/phpstan": "2.1.x-dev"
    },
    "scripts": {
        "stradow-analyze": "vendor/bin/phpstan analyse src",
        "stradow-test": "vendor/bin/phpunit --bootstrap vendor/autoload.php tests",
        "stradow-migrate": "Stradow\\Database\\Migrate::start",
        "stradow-render": "Stradow\\Content\\Controller\\StaticRenderCommand::start",
        "stradow-serve": "@php -S localhost:8000 public/index.php"
    }
}
